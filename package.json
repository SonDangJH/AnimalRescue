  "scripts": {
    "audit": "yarn audit --json | node scripts/audit.js",
    "clean": "rm -rf dist/*",
    "config": "node scripts/config.js",
    "config-check": "node scripts/config.js --check",
    "dev": "export VITE_DEV_API_SERVER_PORT=3500 && concurrently --kill-others \"vite\" \"bun --watch server/apiServer.js\"",
    "dev:ssr": "VITE_ENV=development bun run server/index-dev.js",
    "format": "prettier --write '**/*.{js,css}'",
    "format-ci": "prettier --list-different '**/*.{js,css}'",
    "format-docs": "prettier --write '**/*.md'",
    "test-server": "jest ./server/**/*.test.js",
    "translate": "node scripts/translations.js",
    "build": "bun run build:client && bun run build:server",
    "build:client": "NODE_ENV=production vite build --outDir dist/client",
    "build:server": "NODE_ENV=production vite build --ssr src/entry-server.jsx --outDir dist/server",
    "start": "VITE_ENV=production bun run server/index-prod.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
